}

/**
 * UI关联数据列表
 */
interface IRelationList {
    /**
     * 要显示的UI列表
     */
    show: Array<string>;
    /**
     * 要隐藏的UI列表
     */
    hide: Array<string>;
}

/**
 * UI关联数据
 */
interface IRelationInfo {
    /**
     * 显示时的关联
     */
    show: IRelationList;
    /**
     * 隐藏时的关联
     */
    hide: IRelationList;
}

/**
* GUI 关联关系
*/
